{
    "task": "E2E", 
    "dataset": "mlee",
    "model_type": "GenBEE", 
    "gpu_device": 0, 
    "seed": 0, 
    "cache_dir": "./cache", 
    "output_dir": "./outputs/GenBEE/mlee/e2e", 
    "train_file": "./data/mlee/train.json",
    "dev_file": "./data/mlee/dev.json",
    "test_file": "./data/mlee/test.json",
     

    "pretrained_model_name": "facebook/bart-large",
    "input_style": ["event_type", "event_type_sent", "template"], 
    "output_style": ["trigger:sentence", "argument:sentence"], 
    "n_negative": 8, 
    "max_length": 250,
    "max_output_length": 650,                                      
    "max_epoch": 55,
    "warmup_epoch": 2,
    "accumulate_step": 2,
    "train_batch_size": 12,
    "eval_batch_size": 12,
    "learning_rate": 1e-05,
    "weight_decay": 1e-05,
    "grad_clipping": 5.0,
    "beam_size": 4,
    "ignore_first_header": true,
    "use_encoder_prefix": true,
    "use_cross_prefix": true,
    "use_decoder_prefix": false,
    "prefix_length": 40,
    "latent_dim": 1024,
    "freeze_bart": false,
    "freeze_prefixprojector": false,
    "pretrained_model_path": null
